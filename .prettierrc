{
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "printWidth": 100,
  "trailingComma": "es5",
  "bracketSameLine": false,
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "auto",
  "htmlWhitespaceSensitivity": "css",
  "plugins": ["prettier-plugin-go-template"],
  "overrides": [
    {
      "files": ["*.html", "*.gohtml", "*.go.html", "*.tmpl"],
      "options": {
        "parser": "go-template",
        "tabWidth": 2,
        "useTabs": false,
        "singleQuote": false
      }
    }
  ]
}
